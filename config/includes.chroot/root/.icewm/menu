prog "X Terminal" xterm xterm -ls
prog "Web Browser" iceweasel iceweasel
prog "Network Settings" netsettings.sh xterm -e /root/bin/netsettings.sh -geom 88x24
menu "More XTerms..." folder {
    prog "black on white" xterm xterm -ls -fg black -bg white
    prog "white on black" xterm xterm -ls -fg white -bg black
    prog "green on black" xterm xterm -ls -fg green -bg black
    prog "black on white (large font)" xterm xterm -ls -fg black -bg white -fn 10x20
    prog "white on black (large font)" xterm xterm -ls -fg white -bg black -fn 10x20
    prog "green on black (large font)" xterm xterm -ls -fg green -bg black -fn 10x20
    menu "fullscreen" folder {
        prog "black on white" xterm xterm -ls -fg black -bg white -fullscreen
        prog "white on black" xterm xterm -ls -fg white -bg black -fullscreen
        prog "green on black" xterm xterm -ls -fg green -bg black -fullscreen
        prog "black on white (large font)" xterm xterm -ls -fg black -bg white -fn 10x20 -fullscreen
        prog "white on black (large font)" xterm xterm -ls -fg white -bg black -fn 10x20 -fullscreen
        prog "green on black (large font)" xterm xterm -ls -fg green -bg black -fn 10x20 -fullscreen
    }
}
separator
